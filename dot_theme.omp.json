{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "background": "transparent",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#FF5252{{ end }}",
      "#D4E157"
    ],
    "template": "❯ "
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#546E7A",
          "foreground": "#26C6DA",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 2,
            "mapped_locations": {
              "D:\\source\\repos\\github.com": ""
	          }
          },
          "style": "plain",
          "type": "path"
        },
        {
          "type": "git",
          "background": "#546E7A",
          "foreground": "#D4E157",
          "style": "plain",
          "template": "<#26C6DA>\ue0b1 </>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "properties":{
            "mapped_branches": {
              "feat/*": "\uF135/",
              "feature/*": "\uF135/",
              "bug/*": "\uF188/",
              "bugfix/*": "\uF188/",
              "experimental/*": "\uF0C3/"
            }
          }
        },
        {
          "background_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ else }}#D4E157{{ end }}"
          ],
          "foreground": "#546E7A",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue0b0",
          "type": "status"
        },
        {
          "background": "transparent",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ else }}#D4E157{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue0b0 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " {{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#546E7A",
          "foreground": "#26C6DA",
          "style": "plain",
          "template": "\ue0b3 {{ dateInZone \"15:04:05Z\" .CurrentDate \"UTC\" }} ",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "version": 3
}
